{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "material-ui": "^0.20.2",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "gen": "plop --plopfile ./gen-cli/plopfile.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:a": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:f": "pretty-quick --staged",
    "precommit": "npm run format:a",
    "git:template": "git config --global commit.template ./.git-commit-message.txt"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "husky": "^1.3.1",
    "plop": "^2.3.0",
    "prettier": "^1.16.4",
    "redux-devtools": "^3.5.0"
  }
}
